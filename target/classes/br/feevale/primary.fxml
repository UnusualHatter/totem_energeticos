<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.control.Button?>
<?import javafx.geometry.Insets?>
<?import javafx.geometry.Pos?>

<VBox alignment="CENTER" spacing="20" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="br.feevale.PrimaryController">

    <GridPane fx:id="gridProdutos" hgap="20" vgap="20">
        <VBox.margin>
            <Insets top="20"/>
        </VBox.margin>
    </GridPane>

    <Button text="Ir para Carrinho" onAction="#switchToSecondary"/>
</VBox>
